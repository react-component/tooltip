{
  "name": "rc-tip",
  "version": "1.0.0",
  "description": "tip ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-tip",
    "tip"
  ],
  "homepage": "http://github.com/react-component/tip",
  "author": "sliuqin@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/tip.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/tip/issues"
  },
  "licenses": "MIT",
  "spm": {
    "dependencies": {
      "react": "*"
    },
    "buildArgs": "--global react:window.React"
  },
  "config":{
    "port": 8001
  },
  "scripts": {
    "example": "webpack -d && rc-tools run jsx2html",
    "less": "rc-tools run less",
    "history": "rc-tools run history",
    "start": "node --harmony node_modules/.bin/rc-server",
    "publish": "spm publish && rc-tools run tag",
    "lint": "rc-tools run lint",
    "test": "",
    "saucelabs": "rc-tools run saucelabs",
    "browser-test": "rc-tools run browser-test",
    "browser-test-cover": "rc-tools run browser-test-cover"
  },
  "devDependencies": {
    "css-loader": "^0.9.1",
    "expect.js": "~0.3.1",
    "file-loader": "^0.8.1",
    "jsx-loader": "^0.12.2",
    "precommit-hook": "^1.0.7",
    "rc-server": "^2.0.0",
    "rc-tools": "^1.1.0",
    "react": "~0.12.1",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "~1.5.3"
  },
  "precommit": [
    "lint",
    "less"
  ]
}
